
// P06 Vowel Movement 1.cpp : This file contains the 'main' function. Program execution begins and ends there.

#include <iostream>
#include <string>
#include <cstring>

using namespace std;

int main()
{
    string mySentence;

    cout << "Type a short sentence and we'll have a vowel movement.\n";
    getline(cin, mySentence);
    cout << "\n" << mySentence << "\n";
    // Add more stuff here! 
    return 0;
}
